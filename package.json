{
  "name": "voice-activated-ai-agent",
  "version": "1.0.0",
  "description": "Voice Activated AI Agent with WebSocket support",
  "main": "dist/main.js",
  "type": "module",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "devDependencies": {
    "@eslint/js": "~9.17",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "~8.42",
    "@types/express": "^4.17.21",
    "@types/node": "~20",
    "@types/nodemailer": "^6.4.17",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/parser": "~8.19",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "~9.17",
    "eslint-config-prettier": "~9.1",
    "globals": "~15.14",
    "prettier": "~3.4.2",
    "rimraf": "~6.0",
    "ts-api-utils": "~2.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.7",
    "typescript-eslint": "~8.19",
    "vitest": "^3.1.2"
  },
  "scripts": {
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/main.ts",
    "frontend": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/frontend/main.ts",
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint .",
    "prettier": "prettier \"src/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"src/**/*.{ts,mts}\" --config .prettierrc --check"
  },
  "author": "Jakub Synowiec <jsynowiec@users.noreply.github.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/speech": "^7.0.1",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "firebase": "^11.6.1",
    "googleapis": "^148.0.0",
    "groq-sdk": "^0.19.0",
    "node-record-lpcm16": "^1.0.1",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.1.9",
    "openai": "^4.95.1",
    "redis": "^4.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.14.2"
  },
  "volta": {
    "node": "22.12.0"
  }
}
